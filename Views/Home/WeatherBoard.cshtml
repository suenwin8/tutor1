@model tutor1.Models.Entity.Weather

@{
}


<div class="alert alert-success">
    <dl class="dl-horizontal">
        <dt>日期</dt>
        <dd>@Model.asDate.ToShortDateString()</dd>
        <dt>地區</dt>
        <dd>
            @Model.Location
        </dd>
        <dt>溫度</dt>
        <dd>@Model.Temperature</dd>
        <dt>濕度</dt>
        <dd>@Model.Humidity</dd>
        <dt>天氣</dt>
        <dd>@Model.Status</dd>
    </dl>
</div>